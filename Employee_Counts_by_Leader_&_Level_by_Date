--Count of Employees as of VARIABLE_DATE - 10,520 on 4/26/2023, 11,050 on 4/26/2022
--Can be modified to look at different levels
--If there is a vacant plot of the heirarchy on the level you are running the total will be less than the company total
SELECT
    :effdt AS "As of",
    "Lvl 1 PosID",          --Change level here/7 changes needed
        "Lvl 1 Description",    --Change level here
        "Leader Name",
    "Level",
    "Emp Count"
FROM
    (
        SELECT
            "Lvl 1 PosID",          --Change level here
                        "Lvl 1 Description",    --Change level here
                        CASE
                WHEN jobs.grade = '0'      THEN
                    '1 - Entry-level and lower-level employees'
                WHEN jobs.grade = '1'      THEN
                    '1 - Entry-level and lower-level employees'
                WHEN jobs.grade = '2'      THEN
                    '1 - Entry-level and lower-level employees'
                WHEN jobs.grade = '3'      THEN
                    '1 - Entry-level and lower-level employees'
                WHEN jobs.grade = '4'      THEN
                    '1 - Entry-level and lower-level employees'
                WHEN jobs.grade = '5'      THEN
                    '1 - Entry-level and lower-level employees'
                WHEN jobs.grade = '6'      THEN
                    '1 - Entry-level and lower-level employees'
                WHEN jobs.grade = '7'      THEN
                    '2 - Mid-level employees and/or technical workers'
                WHEN jobs.grade = '8'      THEN
                    '2 - Mid-level employees and/or technical workers'
                WHEN jobs.grade = '12'     THEN
                    '2 - Mid-level employees and/or technical workers'
                WHEN jobs.grade = '13'     THEN
                    '2 - Mid-level employees and/or technical workers'
                WHEN jobs.grade = '14'     THEN
                    '2 - Mid-level employees and/or technical workers'
                WHEN jobs.grade = '15'     THEN
                    '3 - Mid-level managers and supervisors'
                WHEN jobs.grade = '16'     THEN
                    '3 - Mid-level managers and supervisors'
                WHEN jobs.grade = '17'     THEN
                    '3 - Mid-level managers and supervisors'
                WHEN jobs.grade = '18'     THEN
                    '3 - Mid-level managers and supervisors'
                WHEN jobs.grade = '19'     THEN
                    '3 - Mid-level managers and supervisors'
                WHEN jobs.grade = '20'     THEN
                    '4 - Senior managers and supervisors'
                WHEN jobs.grade = '21'     THEN
                    '4 - Senior managers and supervisors'
                WHEN jobs.grade = '22'     THEN
                    '4 - Senior managers and supervisors'
                WHEN jobs.grade = '26'     THEN
                    '2 - Mid-level employees and/or technical workers'
                WHEN jobs.grade = '30'     THEN
                    '1 - Entry-level and lower-level employees'
