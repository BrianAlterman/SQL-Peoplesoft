                WHEN jobs.grade = '541'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '542'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '543'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '544'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '545'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '546'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '547'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '548'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '549'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '550'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '592'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = '593'    THEN
                    '5 - C-Suite, Board of Directors members, presidents and vice presidents'
                WHEN jobs.grade = 'T75'    THEN
                    '1 - Entry-level and lower-level employees'
                ELSE
                    'Unknown'
            END                      AS "Level",
            COUNT(jobs.emplid)       AS "Emp Count"
        FROM
            ps_job jobs,
            (
                SELECT
                    coalesce("Lvl 9 PosID", "Lvl 8 PosID", "Lvl 7 PosID", "Lvl 6 PosID", "Lvl 5 PosID",
                             "Lvl 4 PosID",
                             "Lvl 3 PosID",
                             "Lvl 2 PosID",
                             "Lvl 1 PosID") AS "Position",
                    "Lvl 0 PosID",
                    "Lvl 1 Effective",
                    "Lvl 1 PosID",
                    "Lvl 1 Description",
                    "Lvl 2 Effective",
                    "Lvl 2 PosID",
                    "Lvl 2 Description",
                    "Lvl 3 Effective",
                    "Lvl 3 PosID",
                    "Lvl 3 Description",
                    "Lvl 4 Effective",
                    "Lvl 4 PosID",
                    "Lvl 4 Description",
                    "Lvl 5 Effective",
                    "Lvl 5 PosID",
                    "Lvl 5 Description",
                    "Lvl 6 Effective",
                    "Lvl 6 PosID",
                    "Lvl 6 Description",
                    "Lvl 7 Effective",
                    "Lvl 7 PosID",
                    "Lvl 7 Description",
                    "Lvl 8 Effective",
                    "Lvl 8 PosID",
                    "Lvl 8 Description",
                    "Lvl 9 Effective",
                    "Lvl 9 PosID",
                    "Lvl 9 Description"
                FROM
                    (
                        SELECT
                            pos.reports_to      AS "Lvl 0 PosID",
                            pos.effdt           AS "Lvl 1 Effective",
                            pos.position_nbr    AS "Lvl 1 PosID",
                            pos.descr           AS "Lvl 1 Description",
                            NULL                AS "Lvl 2 Effective",
                            NULL                AS "Lvl 2 PosID",
                            NULL                AS "Lvl 2 Description",
                            NULL                AS "Lvl 3 Effective",
                            NULL                AS "Lvl 3 PosID",
                            NULL                AS "Lvl 3 Description",
                            NULL                AS "Lvl 4 Effective",
                            NULL                AS "Lvl 4 PosID",
                            NULL                AS "Lvl 4 Description",
                            NULL                AS "Lvl 5 Effective",
