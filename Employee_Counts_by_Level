            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '532'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '533'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '534'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '535'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '536'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '537'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '538'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '539'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '540'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '541'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '542'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '543'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '544'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '545'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '546'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '547'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '548'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '549'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '550'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '592'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = '593'    THEN
            '5 - C-Suite, Board of Directors members, presidents and vice presidents'
        WHEN jobs.grade = 'T75'    THEN
            '1 - Entry-level and lower-level employees'
        ELSE
            'Unknown'
    END AS "Level",
    COUNT(jobs.emplid)
FROM
    ps_job jobs
WHERE
        jobs.per_org = 'EMP'
    AND jobs.empl_status IN ( 'A', 'L', 'P' )
    AND jobs.action_reason <> 'ACQ'
    AND jobs.effseq = (
        SELECT
            MAX(jobs3.effseq)
        FROM
            ps_job jobs3
        WHERE
                jobs.emplid = jobs3.emplid
            AND jobs3.effdt = jobs.effdt
    )
    AND jobs.effdt = (
        SELECT
            MAX(jobs2.effdt)
        FROM
            ps_job jobs2
        WHERE
                jobs.emplid = jobs2.emplid
            AND jobs2.effdt <= :as_of_date
    )
GROUP BY
    CASE
        WHEN jobs.grade = '0'      THEN
            '1 - Entry-level and lower-level employees'
        WHEN jobs.grade = '1'      THEN
            '1 - Entry-level and lower-level employees'
        WHEN jobs.grade = '2'      THEN
            '1 - Entry-level and lower-level employees'
        WHEN jobs.grade = '3'      THEN
            '1 - Entry-level and lower-level employees'
        WHEN jobs.grade = '4'      THEN
            '1 - Entry-level and lower-level employees'
        WHEN jobs.grade = '5'      THEN
            '1 - Entry-level and lower-level employees'
        WHEN jobs.grade = '6'      THEN
            '1 - Entry-level and lower-level employees'
        WHEN jobs.grade = '7'      THEN
            '2 - Mid-level employees and/or technical workers'
        WHEN jobs.grade = '8'      THEN
            '2 - Mid-level employees and/or technical workers'
        WHEN jobs.grade = '12'     THEN
            '2 - Mid-level employees and/or technical workers'
        WHEN jobs.grade = '13'     THEN
            '2 - Mid-level employees and/or technical workers'
        WHEN jobs.grade = '14'     THEN
            '2 - Mid-level employees and/or technical workers'
        WHEN jobs.grade = '15'     THEN
            '3 - Mid-level managers and supervisors'
        WHEN jobs.grade = '16'     THEN
            '3 - Mid-level managers and supervisors'
        WHEN jobs.grade = '17'     THEN
            '3 - Mid-level managers and supervisors'
        WHEN jobs.grade = '18'     THEN
            '3 - Mid-level managers and supervisors'
        WHEN jobs.grade = '19'     THEN
            '3 - Mid-level managers and supervisors'
        WHEN jobs.grade = '20'     THEN
